<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title><%= htmlWebpackPlugin.options.title %></title>
  <% if (htmlWebpackPlugin.options.files.css) { %>
  <% htmlWebpackPlugin.options.files.css.forEach(function(name){ %>
  <link rel="stylesheet" type="text/css" href="<%= name %>"> <% }) %>
  <% } %>
</head>
<body>
<div id="container" class="animated fadeIn">
  <box>
    <box-filter params="title:'edit_title'">
      <div class="pull-right">
        <button class="btn btn-primary btn-sm  m-r-sm" data-bind="click:$root.save2"><span class="fa fa-save"></span>&nbsp;保存</button>
        <a href="../list/index.html" class="btn btn-outline btn-sm m-r-sm"><span class="fa fa-reply"></span>&nbsp;返回</a>
      </div>
    </box-filter>
    <box-content params="title: $root.title" id="a1">
    <form-ctn class="u-row" >
      <form-group params="md:10">

        <form-title params="md:2,sm:2,xs:4">专家:</form-title>
        <form-ctn params="md:2,sm:2,xs:4">
          <u-input params='vm:{data:$root.model},umeta:{"id":"require","required":"true","type":"string","field":"id","nullMsg":"带校验输入框不能为空"}'></u-input>
        </form-ctn>
        <form-title params="md:2,sm:2,xs:4">姓名:</form-title>
        <form-ctn params="md:2,sm:2,xs:4">
          <u-input params='vm:{data:$root.model},umeta:{"id":"require","required":"true","type":"string","field":"expertname","nullMsg":"带校验输入框不能为空"}'></u-input>
        </form-ctn>
       
         <form-title params="md:2,sm:2,xs:4">所属机构:</form-title>
        <form-ctn params="md:2,sm:2,xs:4">
          <u-input params='vm:{data:$root.model},umeta:{"field":"deptname"}'>
          </u-input>
        </form-ctn>

        <form-title params="md:2,sm:2,xs:4">手机:</form-title>
        <form-ctn params="md:2,sm:2,xs:4">
          <u-input params='vm:{data:$root.model},umeta:{"id":"udatetime222","field":"telephone"}'></u-input>
        </form-ctn>

        <form-title params="md:2,sm:2,xs:4">邮箱:</form-title>
        <form-ctn params="md:2,sm:2,xs:4">
          <u-input params='vm:{data:$root.model},umeta:{"id":"require","required":"true","type":"string","field":"email","nullMsg":"带校验输入框不能为空"}'></u-input>
        </form-ctn>

        <form-title params="md:2,sm:2,xs:4">所属公司:</form-title>
        <form-ctn params="md:2,sm:2,xs:4">
          <u-input params='vm:{data:$root.model},umeta:{"field":"companyname"}'>
          </u-input>
        </form-ctn>

      <!--  <form-title params="md:2,sm:2,xs:4">专业:</form-title>
        <form-ctn params="md:4,sm:4,xs:4">
         <u-select params='vm:{data:$root.model},umeta:{"field":"major"}'>
          </u-select>
        </form-ctn>-->
         <div class="u-form-group">
            <label class="u-col-2  text-right padding-5  u-form-label">
             专业:
            </label>
            <div class="u-col-10">

                    <div class="majors">
                    <u-input params='vm:{data:$root.model},umeta:{"id":"require","required":"true","type":"string","field":"major","nullMsg":"带校验输入框不能为空"}'></u-input>

                    </div>
            </div>
            <div class="u-col-10">
              <div class="padding-top-10">
                <span>可选专业:</span>
                <!-- ko foreach:store.rows() -->
                  <span class="major_type" data-bind="text:$data.ref('majorName'),click:$parent.changeType,css:{ismain:$data.ref('isMainPro')()==1}"></span>
                <!-- /ko -->
                <i class="icon uf uf-pencil" data-bind="click:major"></i>
              </div>
            </div>

          </div>
        
      </form-group>
    </form-ctn>
  </box>
</div>

<% if (htmlWebpackPlugin.options.files.js) { %>
<% htmlWebpackPlugin.options.files.js.forEach(function(name){ %>
<script type="text/javascript" src="<%= name %>"></script><% }) %>
<% } %>
</body>
</html>
